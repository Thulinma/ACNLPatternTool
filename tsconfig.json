{
  "compilerOptions": {
    "target": "ES5",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "build",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "Injected/*": [
        "./injected/*",
      ],
    },
    "downlevelIteration": true,
    "lib": [
      "DOM",
      "ESNext",
    ]
  }
}